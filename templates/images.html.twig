<div class="form-widget">
    {% for image in field.value %}
        <a href="#" class="ea-lightbox-thumbnail" style="width: 50px; height: auto;" data-featherlight="#ea-lightbox-{{ image.id }}" data-featherlight-close-on-click="anywhere">
                <img src="/resources/images/{{ image.image }}">
        </a>

            <div id="ea-lightbox-{{ image.id }}" class="ea-lightbox">
                  <img src="{{ image.imageURL }}">
            </div>
        {% endfor %}

</div>